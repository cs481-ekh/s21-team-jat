<style>
table, th, td {
  border: 1px solid black;
  border-collapse: collapse;
}
th, td {
  padding: 15px;
}
</style>

<table id="user-table">
  <tr>
    <th>Username:</th>
    <th>Password:</th>
  </tr>
  <% users.forEach(function(element, index){ %>
    <tr>
      <td><%= element.username %></td>
      <td><%= element.password %></td>
    </tr>
  <% }); %> 
</table>